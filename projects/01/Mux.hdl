/**
 * Multiplexor. If sel == 1 then out = b else out = a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
     Not(in=sel, out=notSel);
     And(a=a, b=notSel, out=aOut);
     And(a=b, b=sel, out=bOut);
     Or(a=aOut, b=bOut, out=out);
}
